<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">


<!-- 参考：https://blog.csdn.net/testcs_dn/article/details/79295065 -->
<generatorConfiguration>
	<properties resource="application.properties" />

	<context id="fishingHomeTables" defaultModelType="flat"
		targetRuntime="MyBatis3">


		<commentGenerator>
			<!-- 默认是flase，mybatis generator通过注释来知道代码是不是自己生成的，
			重新生成时候只覆盖自己生成的那部分，设置为true则相当于没法知道哪些是自动生成的，哪些是后来加的。
			这里一定要是false -->
			<!--<property name="suppressAllComments" value="true" /> -->
			<!-- <property name="suppressDate" value="true" /> -->
		</commentGenerator>

		<jdbcConnection
			driverClass="${spring.datasource.driver-class-name}"
			connectionURL="${spring.datasource.url}"
			userId="${spring.datasource.username}"
			password="${spring.datasource.password}" />




		<javaModelGenerator
			targetPackage="${mybatis.typeAliasesPackage}"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator
			targetPackage="${mybatis.mapperPackage}"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="${mybatis.daoPackage}"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

		<table schema="fishing_home" tableName="user_account"
			domainObjectName="UserAccount">
			<generatedKey column="user_id" sqlStatement="MYSQL"
				identity="true" />
				<columnOverride column="user_id" javaType="long" />
		</table>

	</context>
</generatorConfiguration>